  "repository": {
    "type": "git",
    "url": "https://github.com/FELS-dev/backend"
  },
  "homepage": "",
  "author": "",
  "dependencies": {
    "commander": "^9.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.2",
    "mysql2": "^3.2.3",
    "sequelize": "^6.31.0",
    "simple-node-logger": "^21.8.12"
  },
  "devDependencies": {
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "supertest": "^6.1.6"
